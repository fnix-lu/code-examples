/* pages/order/paymentFeedback/components/UploadFile/UploadFile.wxss */
.file-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  /* margin-top: -32rpx; */
}
/* .file-list::after {
  content: '';
  width: 208rpx;
} */

.file-box {
  /* width: 208rpx; */
  /* height: 208rpx; */
  /* margin-top: 32rpx; */
  position: relative;
  box-sizing: border-box;
  border-radius: 8rpx;
  border: 1rpx solid #CCCCCC;
}
.file-box .file-item {
  width: 100%;
  height: 100%;
  display: block;
}
.file-box .i-del {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: -16rpx;
  top: -16rpx;
}

.file-upload {
  display: flex;
  justify-content: center;
  align-items: center;
  border: none;
  background: #F7F8FA;
}
.file-upload .i-upload {
  width: 48rpx;
  height: 48rpx;
}